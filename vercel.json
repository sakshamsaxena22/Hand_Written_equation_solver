{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.10",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10"
  }
}

